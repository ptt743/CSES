#include<iostream>
#include<vector>
#include<algorithm>
#include<string>
#include<queue>
#include<stack>
#include<set>
#include<unordered_map>
#include<map>
#include<cmath>
#include<functional>
#define ll long long

using namespace std;
//*****taipt*****//
/*
*/
void solve(){
	int n, k;
	cin>> n >> k;
	vector<int> nums(n);
	for(int i =0;i< n;i++) cin>> nums[i];

	unordered_map<int,int> mp;
	for(int i =0;i< k;i++) mp[nums[i]]++;
	int curr = 0;
	while(curr<=k){
		if(mp[curr]==0)break;
		curr++;
	}

	for(int i =k ;i< n;i++){
		mp[nums[i-k]]--;
		mp[nums[i]]++;
		if(mp[nums[i-k]]==0 && nums[i-k]<curr) curr = nums[i-k];
		while(mp[curr]>0) curr++;
	}
	cout<< curr<<endl;
}
 
int main() {
    ios_base::sync_with_stdio(0); cin.tie(0);
    solve();
    return 0;
}
